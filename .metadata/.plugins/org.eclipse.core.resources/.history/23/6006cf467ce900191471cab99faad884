package action.blog;

import action.Action;

public class BlogFactory {
	public static Action getAction(String cmd) {
		if(cmd.equals("write")) {//글쓰기 페이지
			
		}else if(cmd.equals("update")) { //수정 페이지
			
		}else if(cmd.equals("delete")) { //삭제 액션
			
		}else if(cmd.equals("list")) { //리스트 페이지
			
		}else if(cmd.equals("view")) { // 상세보기 페이지
			return new BlogViewAction();
		}
		return null;
	}
}
